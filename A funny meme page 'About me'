import 'package:flutter/material.dart';

void main() {
  runApp(MyApp());
}

class MyApp extends StatelessWidget {
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      title: 'About Me',
      theme: ThemeData(
        primarySwatch: Colors.blue,
      ),
      home: MyHomePage(),
    );
  }
}

class MyHomePage extends StatefulWidget {
  @override
  _MyHomePageState createState() => _MyHomePageState();
}

class _MyHomePageState extends State<MyHomePage> {
  // Define a variable to hold the image URL
  String imageUrl = 'data:image/jpeg;base64,';

  // Define a function to change the image URL
  void changeImage() {
    setState(() {
      // Update the image URL
      imageUrl = 'data:image/jpeg;base64,';
    });
  }

  @override
  Widget build(BuildContext context) {
    return Scaffold(
      
      appBar: AppBar(
        title: Text('About Me'),
        centerTitle: true,
      ),
      body: Center(
        child: Column(
          mainAxisAlignment: MainAxisAlignment.center,
          children: [
            // Use the imageUrl variable to display the image
            CircleAvatar(
              radius: 80,
              backgroundImage: NetworkImage(imageUrl),
            ),
            SizedBox(height: 20),
            // Button to change the image
            ElevatedButton(
              onPressed: changeImage,
              child: Text('See The Magic'),
            ),
          ],
        ),
      ),
    );
  }
}
